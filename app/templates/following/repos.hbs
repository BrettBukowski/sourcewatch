<div class="ink-grid vertical-space">
  <h1><i class='ion-star' role='presentation' aria-hidden='true'></i> Starred Projects</h1>

  {{#each}}
  <div class="repo">
    <img {{bind-attr src="owner.avatar_url"}} height="48" width="48" alt="" />
    <a {{bind-attr href="html_url"}}>{{highlight-repo full_name}}</a>

    <div class="summary">
      {{emoji-message description}}
    </div>
  </div>
  {{/each}}
</div>
